<Window x:Class="ContoraRealtor.RegistrManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ContoraRealtor"
        mc:Ignorable="d"
        Title="Регистрация нового менеджера" Height="450" Width="800"
                Icon="C:\Users\RandomUser\source\repos\ContoraRealtor\Resources\img\logo.png"
        ResizeMode="NoResize" Background="#FFCADEEB">
    <Grid>
        <TextBox x:Name="Login" HorizontalAlignment="Left" Height="23" Margin="330,90,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Password" HorizontalAlignment="Left" Height="23" Margin="330,118,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="Назад" HorizontalAlignment="Left" Margin="274,164,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Создать" HorizontalAlignment="Left" Margin="375,164,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Логин" HorizontalAlignment="Left" Margin="274,87,0,0" VerticalAlignment="Top"/>
        <Label Content="Пароль" HorizontalAlignment="Left" Margin="274,118,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
